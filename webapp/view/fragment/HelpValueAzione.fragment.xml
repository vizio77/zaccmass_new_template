<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
    <SelectDialog
        contentHeight=""
        contentWidth=""
        noDataText=""
        rememberSelections="false"
        showClearButton="false"
        title="Cerca"
        cancel="onCancelFragment"
        items="{shModel>/ZES_AZIONE_SET}"
        confirm="onPressChoiceStandardlist"
    >
        <items>
            <StandardListItem
                activeIcon=""
                adaptTitleSize="true"
                description="{shModel>DESC_BREVE}"
                icon=""
                iconDensityAware="true"
                iconInset="true"
                info=""
                infoState="None"
                infoTextDirection="Inherit"
                title="{shModel>CODICE_AZIONE}"
                titleTextDirection="Inherit"
            />
        </items>
        <customData>
            <c:CustomData key="Property" value="CODICE_AZIONE" writeToDom="false" />
            <c:CustomData key="AZIONE" value="DESC_BREVE" writeToDom="false" />
        </customData>
    </SelectDialog>
</c:FragmentDefinition>