{
  "_version": "1.17.0",
  "sap.app": {
    "id": "zsap.com.r3.cobi.s4.accmass",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.6.7",
      "toolsId": "1c9c1f5f-09ad-44aa-88f6-b17c37070482"
    },
    "dataSources": {
      "sapHanaS2": {
        "uri": "/sap/opu/odata/sap/ZSS4_R3_FORM_003_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
			"accantonamenti": {
				"uri": "/sap/opu/odata/sap/ZSS4_COBI_ACCANTONAM_FORM_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/accantonamenti.xml"
				}
			},
			"odataSH": {
				"uri": "/sap/opu/odata/sap/ZSS4_CO_GEST_TIPOLOGICHE_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/odataSH.xml"
				}
			}
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
			"viewName": "zsap.com.r3.cobi.s4.accmass.view.App",
			"type": "XML"
		},
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.71.13",
      "libs": {
        "sap.collaboration": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zsap.com.r3.cobi.s4.accmass.i18n.i18n"
        }
      },
			"odataSH": {
				"dataSource": "odataSH"
			},
			"sapHanaS2": {
				"dataSource": "sapHanaS2"
			}
			,
			"accantonamenti": {
				"dataSource": "accantonamenti"
			}
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "zsap.com.r3.cobi.s4.accmass.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
			"routes": [
				{
					"name": "RouteView",
					"pattern": "",
					"target": [
						"TargetView"
					]
				},
				{
					"name": "Home",
					"pattern": "Home",
					"target": [
						"Home"
					]
				},
				{
					"name": "Detail",
					"pattern": "Detail",
					"target": [
						"Detail"
					]
				},
				{
					"name": "Gestione",
					"pattern": "Gestione",
					"target": [
						"Gestione"
					]
				}
			],
      "targets": {        
        "TargetView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Home"
				},
				"Home": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Home"
				},
				"Detail": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Detail"
				},
				"Gestione": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Gestione"
				}
      }
    }
  }
}
